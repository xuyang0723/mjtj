.main_mzb {
    box-sizing: border-box;
    width: 100%;
    height: 100%;
    padding-top: 80rpx;
    background-color: #F7F7F7;
    -webkit-overflow-scrolling: touch;
}
.main_header_box {
    box-sizing: border-box;
    position: fixed;
    top: 0;
    z-index: 600;
    width: 100%;
    height: 60rpx;
    background:#fff;
    color: #333;
    font-size: 28rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    margin-bottom: 22rpx;
}
.main_header_title {
    width: 20%;
    height: 100%;
    font-size: 28rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}
.main_header_title_text{
  display: block;
  line-height: 1.5;
}
